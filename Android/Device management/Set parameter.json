            "description": "Changes values of certain parameters on the card reader.",
            "parameters": [
                {
                    "name": "param",
                    "type": "DeviceParameter",
                    "validation": "Required",
                    "notes": "The name of the parameter to change"
                },
                {
                    "name": "value",
                    "type": "String",
                    "validation": "Required",
                    "notes": "New value of the parameter"
                },
                {
                    "name": "device",
                    "type": "Device",
                    "validation": "Optional",
                    "notes": "This parameter specifies to the system which device you want to use for the operations. If none is supplied the system will attempt to use a default device, if any."
                }
            ],
